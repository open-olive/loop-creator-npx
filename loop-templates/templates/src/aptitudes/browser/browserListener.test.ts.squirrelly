import { getInsuranceInfo } from './browserListener';

jest.mock('@oliveai/ldk');

const browserSpy = jest.fn().mockImplementationOnce(() => Promise.resolve());

jest.mock('./cursorListener', () => ({
  getInsuranceInfo: async () => {
    browserSpy();
  }
}));

describe('Browser Listener', () => {
  afterEach(() => {
    jest.clearAllMocks();
  });

 

  it('should call the browser listener', async () => {
    await getInsuranceInfo();

    expect(browserSpy).toBeCalled();
  });

 
});