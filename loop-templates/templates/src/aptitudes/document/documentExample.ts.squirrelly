import { document, filesystem } from "@oliveai/ldk";
import { DocumentWhisper } from "../../whispers/";

const run = async () => {
  const filePath = '/Users/aleciberg/Downloads/Untitled spreadsheet.xlsx'
  const bytes = await filesystem.readFile(filePath);
  const workbook = await document.xlsxDecode(bytes);

  await DocumentWhisper.show(JSON.stringify(workbook));
};

export default { run };
