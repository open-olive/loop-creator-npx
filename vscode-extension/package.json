{
  "name": "vscode-loop-development-kit",
  "displayName": "Loop Development Kit",
  "description": "Extension to generate boiler plate code for loops",
  "publisher": "Olive-AI",
  "icon": "images/oliveLogo.png",
  "version": "0.3.2",
  "engines": {
    "vscode": "^1.54.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/open-olive/vscode-loop-development-kit",
    "directory": "vscode-extension"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:loop-development-kit.createLoop"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "loop-development-kit.createLoop",
        "title": "Create Loop"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@oliveai/loop-templates": "*",
    "@types/fs-extra": "^9.0.8",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "raw-loader": "^4.0.2",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode": "^0.9.9",
    "vscode-test": "^1.5.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "@types/squirrelly": "^7.1.1",
    "fs-extra": "^9.1.0",
    "squirrelly": "^8.0.8"
  }
}